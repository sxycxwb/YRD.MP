
@{
    ViewBag.Title = "营业明细--支付单详情";
}

@section css{
    <link rel="stylesheet" href="@ConfigHelper.GetCdnUrl/Content/css/account-manage.css" />
    <!--本页.css-->
    <link rel="stylesheet" href="@ConfigHelper.GetCdnUrl/Content/css/iconbg.css" />
    <link rel="stylesheet" href="@ConfigHelper.GetCdnUrl/Content/css/common.css" />
    <link rel="stylesheet" href="@ConfigHelper.GetCdnUrl/Content/css/operation_manage.css" />
}
<div class="page-content">
    <div class="container-fluid">
        <!--面包屑-->
        <div class="row-fluid">
            <div class="span12">
                <ul class="breadcrumb">
                    <li class="backprev">
                        <a href="javascript:history.back();">返回上一级</a>
                    </li>
                    <li>
                        <b class="iconbg txt"></b>
                        <a>经营管理</a>
                        <b class="iconbg angle-right-solid"></b>
                    </li>
                    <li>
                        <a href="/BusinessRecords/OmOperationRecord">营业记录</a>
                        <b class="iconbg angle-right-solid"></b>
                    </li>
                    <li>
                        <a href="/BusinessRecords/OmOrOperationDetail">营业明细</a>
                        <b class="iconbg angle-right-solid"></b>
                    </li>
                    <li>
                        <a>支付单详情</a>
                    </li>
                </ul>
            </div>
        </div>
        <!--内容区域-->
        <div class="row-fluid" id="om_payDetail">
            <div class="row-fluid">
                <!-- 支付单详情-->
                <div class="span6 pdContainer payDetailContainer">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <td colspan="3">支付单详情</td>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>支付订单</td>
                                <td colspan="2">@ViewBag.ID</td>
                            </tr>
                            <tr>
                                <td>结账时间</td>
                                <td colspan="2">@ViewBag.CreateTime</td>
                            </tr>
                            <tr>
                                <td>实收金额</td>
                                <td colspan="2" class="c06c"><span>@ViewBag.Payable</span>元</td>
                            </tr>
                            <tr>
                                <td>餐桌</td>
                                <td colspan="2" class="f60">@ViewBag.TableName</td>
                            </tr>
                            <tr>
                                <td>收银</td>
                                <td colspan="2">@ViewBag.OrderSource</td>
                            </tr>
                            <tr>
                                <td>收银员</td>
                                <td colspan="2">@ViewBag.SellerName</td>
                            </tr>
                            <tr>
                                <td>菜品金额</td>
                                <td colspan="2"><span>@ViewBag.TotalPrice</span>元</td>
                            </tr>
                            <tr>
                                <td>优惠条件</td>
                                <td colspan="2">@ViewBag.DiscountName</td>
                            </tr>
                            <tr>
                                <td>优惠金额</td>
                                <td colspan="2"><span>@ViewBag.CardDiscountAmount</span>元</td>
                            </tr>
                            <tr>
                                <td>折扣</td>
                                <td colspan="2"><span>@ViewBag.DiscountScale</span>折</td>
                            </tr>
                            <tr>
                                <td>抹零</td>
                                <td colspan="2"><span>@ViewBag.SmallDiscount</span>元</td>
                            </tr>
                            <tr>
                                <td>应收金额</td>
                                <td colspan="2" class="c06c"><span>@ViewBag.TotalPrice</span>元</td>
                            </tr>
                            <tr>
                                <td>预收订金</td>
                                <td colspan="2"><span>@ViewBag.ResourMoney</span>元</td>
                            </tr>
                            <tr>
                                <td>余额</td>
                                <td colspan="2"><span>@ViewBag.Ye</span>元</td>
                            </tr> 
                            <tr>
                                <td>优惠券</td>
                                <td>@ViewBag.YhqCode</td>
                                <td><span>@ViewBag.YhqMoney</span>元</td>
                            </tr>
                            <tr>
                                <td>餐后券</td>
                                <td>@ViewBag.ChqCode</td>
                                <td><span>@ViewBag.ChqMoney</span>元</td>
                            </tr>
                            <tr>
                                <td>会员卡</td>
                                <td colspan="2" class="f60">@ViewBag.IsUsedCard</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!-- 收款明细-->
                <div class="span6 pdContainer moneyDetailContainer">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <td>收款明细</td>
                            </tr>
                        </thead>
                    </table>
                    <!-- 收款明细-->
                    <div class="mdContent notCardContent">
                        <div class="mdcTime">
                            <div class="mdctTime"><span>首次收款</span></div>
                            <div class="mdctTitle">
                                <span class="mdcttStyle">@ViewBag.FPaymentType</span>
                                ¥<span>@ViewBag.FPayInAmount</span>
                            </div>
                            @if (ViewBag.FPaymentType == "会员卡")
                            {

                                <div class="mdctCont">
                                    <div class="row-fluid">
                                        <div class="span2">姓名：</div>
                                        <div class="span10">@ViewBag.FUserName</div>
                                    </div>
                                    <div class="row-fluid">
                                        <div class="span2">手机：</div>
                                        <div class="span10">@ViewBag.FPhone</div>
                                    </div>
                                    <div class="row-fluid">
                                        <div class="span2">类型：</div>
                                        <div class="span10">@ViewBag.FCardType</div>
                                    </div>
                                    <div class="row-fluid">
                                        <div class="span2">卡号：</div>
                                        <div class="span10">@ViewBag.FCardID</div>
                                    </div>
                                    <div class="row-fluid">
                                        <div class="span2">支付：</div>
                                        <div class="span10">主卡100元，副卡100元</div>
                                    </div>
                                </div>
                            }
                            else if (ViewBag.FPaymentType == "现金")
                            {
                                <div class="row-fluid">
                                    <div class="span2">支付：</div>
                                    <div class="span10">@ViewBag.FAmount</div>
                                </div>
                                <div class="row-fluid">
                                    <div class="span2">找零：</div>
                                    <div class="span10">@ViewBag.FChange</div>
                                </div>
                            }
                            else if (ViewBag.FPaymentType == "微信" || ViewBag.FPaymentType == "支付宝")
                            {
                                <div class="row-fluid">
                                    <div class="span2">账户：</div>
                                    <div class="span10">@ViewBag.FBatchId</div>
                                </div>
                            }
                        </div>
                        @if (ViewBag.IsGuaZhang == "是")
                        {

                            <div class="mdcTime">
                                <div class="mdctTime"><span>二次收款</span></div>
                                <div class="mdctTitle">
                                    <span class="mdcttStyle">挂账</span>
                                    ¥ <span>@ViewBag.Money</span>
                                </div>
                                <div class="mdctCont">
                                    <div class="row-fluid">
                                        <div class="span2">姓名：</div>
                                        <div class="span10">@ViewBag.RepaymentName</div>
                                    </div>
                                    <div class="row-fluid">
                                        <div class="span2">手机：</div>
                                        <div class="span10">@ViewBag.RepaymentPhone</div>
                                    </div>
                                    <div class="row-fluid">
                                        <div class="span2">备注：</div>
                                        <div class="span10">@ViewBag.Remark</div>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section script{
    <script src="@ConfigHelper.GetCdnUrl/Content/js/common.js"></script>
    <script>
        jQuery(document).ready(function () {
            App.init();
            Index.init();
        });
        $(function () {
            $('#loadSidebar').load('loadsidebar.html', function () {
                toggleSidebar();
                jumpSubmenu('yyjl');
            });
        });
    </script>
}
